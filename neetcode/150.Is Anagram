class Solution {
    /**
     * @param {string} s
     * @param {string} t
     * @return {boolean}
     */
    isAnagram(s, t) {

        //change given string to array and sort them
        let s2= s.split("").sort()
        let t2= t.split("").sort()

        //compare the two arrays
        return JSON.stringify(s2)==JSON.stringify(t2);
    }
}


/*Note 


 O(n log n), because the sort() function typically has a time complexity of O(n log n).

*/